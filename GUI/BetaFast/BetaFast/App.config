<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <appSettings>
    <add key ="baseUri" value="127.0.0.1:8080" />
    
    <add key="loginPath" value="/api/account/login" />
    <add key="logoutPath" value="/api/account/logout" />
    
    <add key="registerUserPath" value="/api/account/usermanagement/registeruser" />
    <add key="registerAdminPath" value="/api/account/usermanagement/registeradmin" />
    <add key="deleteUserPath" value="/api/account/usermanagement/delete" />
    <add key="isAdminPath" value="/api/account/usermanagement/isadmin" />
    <add key="updatePasswordPath" value="/api/account/usermanagement/password/update" />
    <add key="allUsersPath" value="/api/account/all" />
    
    <add key="quotePath" value="/api/quotes" />
    
    <add key="posterPath" value="/movies/posters" />
    <add key="allMoviesPath" value="/api/movies/all" />
    <add key="moviePath" value="/api/movies" />
    <add key="addMoviePath" value="/api/movies/add" />
    <add key="deleteMoviePath" value="/api/movies/delete" />

    <add key="cartPath" value="/api/cart"/>
